{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIn": true,
      "debug": false,
      "targets": {
        "browsers": [
          "> 2%",
          "last 2 versions",
          "ie >= 11",
          "ios 9"
        ]
      }
    }],
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-vue-jsx"
  ]
}
